{
  "name": "nextjs-typescript-apollo-boilerplate",
  "version": "0.1.1",
  "private": true,
  "scripts": {
    "dev": "concurrently \"graphql-codegen --watch --config ./.graphqlrc.yml\" \"next dev -p 4600 ./src\"",
    "build": "next build ./src",
    "types:generate": "DEBUG=1 graphql-codegen --config .graphqlrc.yml -r dotenv/config",
    "start": "next start -p $PORT ./src",
    "lint": "next lint",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@apollo/client": "^3.5.5",
    "graphql": "^16.0.1",
    "next": "12.0.4",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.13.12",
    "@graphql-codegen/typescript": "^2.8.2",
    "@graphql-codegen/typescript-operations": "^2.5.7",
    "@graphql-codegen/typescript-react-apollo": "^3.3.6",
    "@svgr/webpack": "^5.5.0",
    "@types/node": "16.11.10",
    "@types/react": "17.0.36",
    "@types/styled-components": "^5.1.15",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "concurrently": "^7.5.0",
    "eslint": "7",
    "eslint-config-next": "12.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "2.4.1",
    "typescript": "4.5.2"
  }
}
